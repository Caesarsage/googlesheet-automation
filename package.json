{
  "name": "google-sheets-automation",
  "version": "0.1.0",
  "description": "Automate Google Sheets from Node.js: add, update, and read rows with simple API. Supports service accounts, header mapping, and batch operations.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unit": "npx jest --testPathIgnorePatterns=integration",
    "test:integration": "npx jest --testPathPattern=integration"
  },
  "keywords": [
    "google-sheets",
    "spreadsheet",
    "api",
    "automation",
     "nodejs",
    "provider"
  ],
  "author": "CaesarSage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaesarSage/google-sheets-automation.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "google-auth-library": "^10.5.0",
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
